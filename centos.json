{
  "builders": [
    {
      "boot_command": [
        "<tab> text ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/centos-6.5/ks.cfg<enter><wait>"
      ],
      "boot_wait": "10s",
      "disk_size": 8192,
      "guest_additions_path": "VBoxGuestAdditions_{{.Version}}.iso",
      "guest_os_type": "RedHat_64",
      "http_directory": "http",
      "iso_checksum": "32c7695b97f7dcd1f59a77a71f64f2957dddf738",
      "iso_checksum_type": "sha1",
      "iso_url": "{{user `mirror`}}/6.5/isos/x86_64/CentOS-6.5-x86_64-bin-DVD1.iso",
      "output_directory": "packer-centos-6.5-x86_64-virtualbox",
      "shutdown_command": "echo 'vagrant'|sudo -S /sbin/halt -h -p",
      "ssh_password": "vagrant",
      "ssh_port": 22,
      "ssh_username": "vagrant",
      "ssh_wait_timeout": "10000s",
      "type": "virtualbox-iso",
      "vboxmanage": [
        [
          "modifyvm",
          "{{.Name}}",
          "--memory",
          "480"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--cpus",
          "1"
        ]
      ],
      "virtualbox_version_file": ".vbox_version",
      "vm_name": "centos-packer"
    },
    {
      "accelerator": "kvm",
      "boot_command": [
        "<tab> text ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/centos-6.5/ks.cfg<enter><wait>"
      ],
      "boot_wait": "10s",
      "disk_size": 8192,
      "format": "qcow2",
      "headless": true,
      "http_directory": "http",
      "iso_checksum": "32c7695b97f7dcd1f59a77a71f64f2957dddf738",
      "iso_checksum_type": "sha1",
      "iso_url": "{{user `mirror`}}/6.5/isos/x86_64/CentOS-6.5-x86_64-bin-DVD1.iso",
      "qemuargs": [
        [
          "-m",
          "1024M"
        ],
        [
          "-cpu",
          "host"
        ],
        [
          "-smp",
          2
        ]
      ],
      "output_directory": "packer-centos-6.5-x86_64-qemu",
      "shutdown_command": "echo 'centos'|sudo -S /sbin/halt -h -p",
      "ssh_password": "centos",
      "ssh_username": "centos",
      "ssh_wait_timeout": "10000s",
      "type": "qemu",
      "vm_name": "centos-packer"
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "inline": ["echo finished"]
    }
  ],
  "variables": {
    "mirror": "http://mirrors.kernel.org/centos"
  }
}
